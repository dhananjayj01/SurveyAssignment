@{
}
@model SectionViewModel

<form method="post">
    @foreach (var q in Model.Questions.OrderBy(x => x.DisplayOrder))
    {
        @await Html.PartialAsync("_QuestionRenderer", q)
    }

    <button type="submit" class="btn btn-primary">
        Submit Section
    </button>
</form>
